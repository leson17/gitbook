{
  "title": "This is my notebook",
  "author": "Leson",
  "discription": "大前端知识点个人总结",
  "plugins": [
    "custom-favicon",
    "-lunr",
    "-search",
    "search-pro",
    "code",
    "summary",
    "disqus",
    "tbfed-pagefooter",
    "back-to-top-button",
    "chapter-fold",
    "expandable-chapters",
    "splitter",
    "lightbox",
    "pageview-count",

    "-sharing",
    "sharing-plus",

    "-highlight",
    "prism",
    "prism-themes",
    "donate",
    "github"
  ],
  "pluginsConfig": {
    "favicon": "favicon/favicon.ico",
    "code": {
      "copyButtons": true
    },
    "disqus": {
      "shortName": "gitbook"
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy github.com/Leson17 2021",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "prism": {
      "css": ["prism-themes/themes/prism-duotone-dark.css"]
    },
    "donate": {
      "wechat": "/images/wxpay.png",
      "alipay": "/images/alipay.png",
      "title": "",
      "button": "赏",
      "alipayText": "支付宝打赏",
      "wechatText": "微信打赏"
    },
    "sharing": {
      "facebook": true,
      "twitter": true,
      "weibo": true,
      "qq": true,
      "google+": true,
      "all": ["douban", "google", "qzone", "linkedin"]
    },
    "github": {
      "url": "https://github.com/leson17"
    }
  }
}
