{
  "title": "This is my notebook",
  "author": "Leson",
  "discription": "大前端知识点个人总结",
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "code",
    "summary",
    "disqus",
    "tbfed-pagefooter",
    "back-to-top-button",
    "chapter-fold",
    "expandable-chapters",
    "splitter",
    "lightbox",
    "pageview-count",

    "-highlight",
    "prism",
    "prism-themes"
  ],
  "pluginsConfig": {
    "code": {
      "copyButtons": true
    },
    "disqus": {
      "shortName": "gitbook"
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy github.com/leson17 2020",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "prism": {
      "css": [
          "prism-themes/themes/prism-duotone-dark.css"
      ]
    }
  }
}
